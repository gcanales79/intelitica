<body>
    <form>

        <div class="form-group col-md-6">
            <label for="inputName">Nombre</label>
            <input type="text" class="form-control" id="cliente" placeholder="Name">
        </div>

        <div class="form-group col-md-6">
            <label for="inputRestaurant">Restaurant</label>
            <input type="text" class="form-control" id="local" placeholder="Restaurante">
        </div>
        <div class="form-group col-md-6">
            <label for="FechadeVisita">Fecha de Visita</label>
            <input type="date" class="form-control" id="fecha_visita" placeholder="Fecha de visita">
        </div>

        <div class="form-group col-md-6">
            <label for="inputTelefono">Telefono</label>
            <input type="number" class="form-control" id="celular" placeholder="Telefono">
        </div>
        <button type="submit" class="btn btn-primary" id="Submit">Submit</button>
    </form>

